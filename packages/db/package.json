{
  "name": "@init/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .nitro .output .turbo .vercel node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "supabase": "supabase",
    "start": "supabase status || supabase start",
    "stop": "supabase stop",
    "reset": "supabase db reset",
    "status": "supabase status",
    "test": "supabase db test",
    "push": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "typegen": "supabase gen types typescript --local > src/database.types.ts",
    "dump": "supabase db dump --local --data-only",
    "migrate": "supabase db migrate",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.45.1",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@kyh/eslint-config": "catalog:",
    "@kyh/prettier-config": "catalog:",
    "@kyh/tsconfig": "catalog:",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "supabase": "^1.187.10",
    "typescript": "catalog:"
  },
  "prettier": "@kyh/prettier-config"
}
